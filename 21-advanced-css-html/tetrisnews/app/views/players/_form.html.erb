<%= form_for @player do |f| %>
	<div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name, class: "form-control" %>
	</div>

	<div class="form-group">
		<%= f.label :score %>
		<%= f.text_field :score, class: "form-control" %>
	</div>

	<div class="form-group">
		<%= f.label :image %>
		<%= f.text_field :image, class: "form-control" %>
	</div>

	<%= f.submit "Create Player", class: "btn btn-primary" %>
<% end %>

